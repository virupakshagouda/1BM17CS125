file1 = open("happy.text", "r")
list1=[]
list1=file1.read().split(",")

#print(list1)


file2 = open("prime.text", "r")
list2=[]
list2=file2.read().split(",")

#print(list2)


a= set(list1)
#print(a)
b= set(list2)
#print(b)
if (a & b): 
    print(a & b)
else: 
    print("No common elements") 

